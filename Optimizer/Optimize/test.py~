dist = 0.0
for i in range(0, 20):
  if(i != 19):
  	dist += math.sqrt( (x[i] - x[i + 1])**2 + (y[i] - y[i + 1])**2) 
  else:
  	dist += math.sqrt( (x[i] - x[0])**2 + (y[i] - y[0])**2) 
print dist
